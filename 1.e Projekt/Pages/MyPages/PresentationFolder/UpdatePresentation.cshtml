@page
@model _1.e_Projekt.Pages.MyPages.PresentationFolder.UpdatePresentationModel
@{
}
<h1>  Update Presentation  </h1>


<form method="post">
    <div class="form-group">
        <label asp-for="@Model.Presentation.PresentationId" class="control-label"></label>
        <input asp-for="@Model.Presentation.PresentationId" class="form-control" />
        <span asp-validation-for="@Model.Presentation.PresentationId" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Presentation.PresentationName class=" control-label"></label>
        <input asp-for="@Model.Presentation.PresentationName" class="form-control" />
        <span asp-validation-for="@Model.Presentation.PresentationName" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Presentation.ImageName" class="control-label"></label>
        <input asp-for="@Model.Presentation.ImageName" class="form-control" />
        <span asp-validation-for="@Model.Presentation.ImageName" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Save" class="btn btn-primary" />
    </div>
</form>
